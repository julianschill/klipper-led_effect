{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "automerge": true,
  "dependencyDashboard": true,
  "automergeType": "branch",
  "branchConcurrentLimit": 3,
  "pip_requirements": {
    "enabled": false
  },
  "packageRules": [
    {
      "matchPackageNames": ["python"],
      "enabled": false
    }
  ]
}
